<link href="all.css" rel="stylesheet" type="text/css">

<header>users information</header>
<hr>
<br/>
<article>
	<form>
		<table class="table">
			<tr class="tr">
				<th class="th">name</td>
				<th class="th">password</td>
				<th class="th">gender</td>
				<th class="th">phone number</td>
				<th class="th">email address</td>
				<th class="th">create date</td>
				<th class="th">update date</td>
			</tr> 
			<% users.forEach(function(user){ %>  
			<tr class="tr">
				<td class="td"><%=user.name %></td>
				<td class="td"><%=user.password %></td>
				<td class="td"><%=user.gender %></td>
				<td class="td"><%=user.phonenumber %></td>
				<td class="td"><%=user.email %></td>
				<td class="td"><%=user.createdate %></td>
				<td class="td"><%=user.updatedate %></td>
			</tr>
			<% }) %> 
		</table>
	</form>
</article>
<hr/>
<footer>users information</footer>




















































<script src="https://code.jquery.com/jquery-1.10.2.js"></script>
<script type="text/javascript">
$(function(){
	// Returns width of browser viewport
	$( window ).width();
	// Returns width of HTML document
	$( document ).width();
	
	$("body").css("padding-left",($(window).width() - $(".login-form").width())/2+"px");
	$("body").css("padding-top",($(window).height() - $(".login-form").height())/2+"px");
});
</script>
