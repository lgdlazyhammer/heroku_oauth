<% layout('layout-super-user') -%>
<% stylesheet('/css/all.css') -%>
<% stylesheet('/css/animate.min.css') -%>
<% stylesheet('/css/bootstrap.min.css') -%>
<% stylesheet('/css/bootstrap-theme.min.css') -%>
<% script('/js/SoundJS/lib/soundjs-0.6.2.min.js') -%>
<% script('/js/PreloadJS/lib/preloadjs-0.6.2.min.js') -%>
<% script('/js/jquery-3.0.0.min.js') -%>
<% script('/js/bootstrap.min.js') -%>
<% script('/js/local-users.js') -%>

<div class="container-fluid">
	<div class="row fill-table-upper-content">&nbsp;
	</div>
	<div id="usersForm" class="row">
		<div class="col-md-1"></div>
		<div class="col-md-10">
			<table class="table table-hover">
				<tbody>
					<tr>
						<th>user name</th>
						<th>email address</th>
						<th>password</th>
						<th>gender</th>
						<th>phone number</th>
						<th>address</th>
						<th>create date</th>
						<th>update date</th>
					<tr>
					<% users.forEach(function(user){ %>  
					<tr class="tr">
						<td class="td"><%=user.name %></td>
						<td class="td"><%=user.password %></td>
						<td class="td"><%=user.gender %></td>
						<td class="td"><%=user.phonenumber %></td>
						<td class="td"><%=user.email %></td>
						<td class="td"><%=user.createdate %></td>
						<td class="td"><%=user.updatedate %></td>
					</tr>
					<% }) %> 
				</tbody>
			</table>
		</div>
		<div class="col-md-1"></div>
	</div>
</div>

